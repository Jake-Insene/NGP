; --------------------------------------------
;         LOGICAL ADD SUB INSTRUCTIONS
; --------------------------------------------
; RDX = Register List
; R10 = Dest Index
; R11 = First Source
; R12 = Second Source
; R13 = Third Source

INCLUDE MacrosWIN64.inc

ADD_SHL_OPC:
    LOGICAL_ADD_SUB ADD, SHL
    JMP CONTINUE_ADDRESS
ADD_SHR_OPC:
    LOGICAL_ADD_SUB ADD, SHR
    JMP CONTINUE_ADDRESS
ADD_ASR_OPC:
