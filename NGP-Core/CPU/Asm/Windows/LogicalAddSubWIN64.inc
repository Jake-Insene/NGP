ADDS_SHL_OPC:
    GET_REGISTER ECX, R11
    GET_REGISTER EDX, R12
    SHIFT_REG EDX, R13D, SHL
    
    CALL compareAdd

    RESTORE_ARGS
    SET_REGISTER EAX, R10

    JMP CONTINUE_ADDRESS
