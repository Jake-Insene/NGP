ORN_SHL_OPC:
    GET_REGISTER R11D, R11
    GET_REGISTER R12D, R12
    SHIFT_REG R12D, R13D, SHL
    
    NOT R12D
    OR R11D, R12D
    SET_REGISTER R11D, R10

    JMP CONTINUE_ADDRESS
