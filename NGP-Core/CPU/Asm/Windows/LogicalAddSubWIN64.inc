    GET_REGISTER ECX, R11
    GET_REGISTER EDX, R12
    SHIFT_REG EDX, R13D, ROR
    
    CALL compareAdd

    RESTORE_ARGS
    MOV DWORD PTR [RDX+R10*4], EAX

