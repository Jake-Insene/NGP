; --------------------------------------------
;         NON BINARY INSTRUCTIONS
; --------------------------------------------
; RDX = Register List
; R10 = Op
; R11 = First Source
; R12 = Second Source

RET_OPC:
    GET_REGISTER R11D, R11
    SET_REGISTER_NO_ZERO R11D, PC_INDEX

    JMP CONTINUE_ADDRESS
BLR_OPC:
    GET_REGISTER R10D, PC_INDEX
    SET_REGISTER_NO_ZERO R10D, LR_INDEX

    GET_REGISTER R11D, R11
    SET_REGISTER_NO_ZERO R11D, PC_INDEX
    JMP CONTINUE_ADDRESS
BR_OPC:
    GET_REGISTER R11D, R11
    SET_REGISTER_NO_ZERO R11D, PC_INDEX
    JMP CONTINUE_ADDRESS
ERET_OPC:
    JMP CONTINUE_ADDRESS
BRK_OPC:
    JMP CONTINUE_ADDRESS
HLT_OPC:
    GET_REGISTER R11D, PSR_INDEX
    OR R11D, HALT_MASK
    SET_REGISTER_NO_ZERO R11D, PSR_INDEX
    JMP CONTINUE_ADDRESS
SIT_OPC:
    JMP CONTINUE_ADDRESS
MSR_OPC:
    JMP CONTINUE_ADDRESS
MRS_OPC:
    JMP CONTINUE_ADDRESS
NOP_OPC:
    ; Do nothing
    JMP CONTINUE_ADDRESS
