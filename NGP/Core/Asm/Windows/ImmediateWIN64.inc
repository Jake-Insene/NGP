; --------------------------------------------
;            IMMEDIATE INSTRUCTIONS
; --------------------------------------------
; RDX = Register List
; R10 = Dest/Source Index
; R12 = Immediate
MOV_IMMEDIATE_OPC:
    MOV DWORD PTR[RDX+R10*4], R12D
    JMP CONTINUE_ADDRESS
MOVT_IMMEDIATE_OPC:
    SHL R12D, 16
    MOV DWORD PTR[RDX+R10*4], R12D
    JMP CONTINUE_ADDRESS
MVN_IMMEDIATE_OPC:
    JMP CONTINUE_ADDRESS
MVNT_IMMEDIATE_OPC:
    JMP CONTINUE_ADDRESS
CMP_IMMEDIATE_OPC:
    MOV ECX, DWORD PTR[RDX+R10*4]
    MOV RDX, R12

    CALL compare

    RESTORE_ARGS

    JMP CONTINUE_ADDRESS
CMN_IMMEDIATE_OPC:
    MOV ECX, DWORD PTR[RDX+R10*4]
    MOV RDX, R12

    CALL compare_add

    RESTORE_ARGS

    JMP CONTINUE_ADDRESS
TST_IMMEDIATE_OPC:
    MOV ECX, DWORD PTR[RDX+R10*4]
    MOV RDX, R12

    CALL compare_and

    RESTORE_ARGS

    JMP CONTINUE_ADDRESS
FMOV_S_IMMEDIATE_OPC:
    JMP CONTINUE_ADDRESS
FMOV_D_IMMEDIATE_OPC:
    JMP CONTINUE_ADDRESS